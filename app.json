{
  "expo": {
    "newArchEnabled": true,
    "name": "foilTribe Adventures",
    "slug": "audifoil",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "scheme": "foiltribe",
    "platforms": [
      "ios",
      "android"
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-video",
      "expo-web-browser",
      "expo-apple-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#0077B6"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.mmdev13.foiltribeadventures",
          "enableGooglePay": true
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mmdev13.foiltribeadventures",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Use the camera to scan payment cards."
      },
      "appleTeamId": "ANAZ4TXPX5"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "predictiveBackGestureEnabled": false,
      "package": "com.mmdev13.foiltribeadventures"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bf9d6b8b-3fac-4a09-828a-51560c2877c9"
      }
    },
    "owner": "mmdev13"
  }
}
