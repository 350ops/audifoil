    "web": {
      "bundler": "metro",
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "router": {
        "origin": "https://foiltribemv.expo.app"
      },
      "eas": {
        "projectId": "360120b8-dba4-4c38-8180-0f2f4fdcfe10"
